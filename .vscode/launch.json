    "version": "0.2.0",
    "configurations": [
        {
            "name": "RunBuild",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\ModBuilder\\build.py",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "RunModBuilderFull",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\ModBuilder\\generalsmodbuilder\\main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--load-default-runner",
                "--load-default-tools",
                "--config-list",
                "${workspaceFolder}\\SampleProject\\ModBundleItems.json",
                "${workspaceFolder}\\SampleProject\\ModBundlePacks.json",
                "${workspaceFolder}\\SampleProject\\ModFolders.json",
                "${workspaceFolder}\\SampleProject\\ModRunner.json",
                "${workspaceFolder}\\SampleProject\\ModChangeLog.json",
                "--make-change-log",
                "--build",
                "--release",
                "--install=ProjectCore",
                "--run",
                "--uninstall",
                "--debug"
            ]
        },
        {
            "name": "RunModBuilderGui",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\ModBuilder\\generalsmodbuilder\\main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--load-default-runner",
                "--load-default-tools",
                "--config-list",
                "${workspaceFolder}\\SampleProject\\ModBundleItems.json",
                "${workspaceFolder}\\SampleProject\\ModBundlePacks.json",
                "${workspaceFolder}\\SampleProject\\ModFolders.json",
                "${workspaceFolder}\\SampleProject\\ModRunner.json",
                "${workspaceFolder}\\SampleProject\\ModChangeLog.json",
                "--make-change-log",
                "--build",
                "--install=ProjectCore",
                "--run",
                "--uninstall",
                "--debug",
                "--gui"
            ]
        },
        {
            "name": "RunModBuilderBuildRelease",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\ModBuilder\\generalsmodbuilder\\main.py",
            "console": "integratedTerminal",
