            "args": [
                "--load-default-runner",
                "--load-default-tools",
                "--config-list",
                "${workspaceFolder}\\SampleProject\\ModBundleItems.json",
                "${workspaceFolder}\\SampleProject\\ModBundlePacks.json",
                "${workspaceFolder}\\SampleProject\\ModFolders.json",
